@import 'assets/colors.styl'
@import 'assets/shapes.styl'

.icon
    rect 32*PX
    cursor  pointer
    position relative

    &.is-not-hoverable
        cursor  default


    &.is-white-bg-on-check
   		&.is-check
   			position: relative
   			&:after
   				content ''
   				width: 83%
   				height: 66%
   				position absolute
   				top  17%
   				left: 8.5%
   				background-color: rgba(255,255,255,.2)
   				border-radius: 2px

    &.is-white-bg-on-check-large
        &.is-check
            position: relative
            &:after
                content ''
                width: 83%
                height: 83%
                position absolute
                top  8.5%
                left: 8.5%
                background-color: rgba(255,255,255,.2)
                border-radius: 2px

    &.is-glow--onhover
        &:before
            content ''
            position absolute
            top 0
            left 0
            rect 100%
            opacity 0
            -webkit-transition: all .17s ease-in-out;
            -moz-transition: all .17s ease-in-out;
            -o-transition: all .17s ease-in-out;
            transition: all .17s ease-in-out;

        &:hover
            &:before 
                opacity 1
    
    &.is-x1
        rect 1em
    &.is-x3
        rect 5em
    &.is-u3
        rect 3*gs
    &.is-u4
        rect 4*gs
    &.is-u6
        rect 6*gs
    &.is-20
